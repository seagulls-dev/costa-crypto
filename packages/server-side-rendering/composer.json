{
    "name": "HolluwaTosin360/server-side-rendering",
    "description": "Server side rendering JavaScript in your PHP application",
    "keywords": [
        "HolluwaTosin360",
        "ssr",
        "server-side-rendering"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/process": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "HolluwaTosin360\\Ssr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HolluwaTosin360\\Ssr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
